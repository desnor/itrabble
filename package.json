{
  "name": "itrabble",
  "version": "2.0.0-rc.1",
  "description": "Library to add useful methods to JavaScript iterables eg. Array, Map, Set, String, TypedArray",
  "browser": "dist/index.umd.js",
  "main": "commonjs/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./pipeable/": "./lib/pipeable/",
    "./commonjs": "./commonjs/index.js",
    "./commonjs/pipeable": "./commonjs/pipeable/index.js"
  },
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rollup -c",
    "prepublishOnly": "rollup -c && npm t",
    "test": "nyc ava",
    "test:watch": "ava -w",
    "generate-docs": "jsdoc lib README.md -d docs",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desnor/itrabble.git"
  },
  "keywords": [
    "iterables",
    "ES6",
    "lazy",
    "streams",
    "generator",
    "iterator",
    "ES2015"
  ],
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "author": "Alex Revell <revellalex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/desnor/itrabble/issues"
  },
  "homepage": "https://github.com/desnor/itrabble#readme",
  "size-limit": [
    {
      "path": "dist/index.umd.js",
      "limit": "10 KB"
    },
    {
      "path": "commonjs/index.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "ava": "^3.13.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.7",
    "husky": "^6.0.0",
    "nyc": "^14.1.1",
    "rollup": "^1.25.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "sinon": "^7.5.0",
    "size-limit": "^4.10.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
