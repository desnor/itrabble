{
  "name": "itrabble",
  "version": "2.0.0-rc.1",
  "description": "Library to add useful methods to JavaScript iterables eg. Array, Map, Set, String, TypedArray",
  "browser": "dist/index.umd.js",
  "main": "dist/commonjs/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./pipeable": "./dist/pipeable/index.js",
    "./commonjs": "./dist/commonjs/index.js",
    "./commonjs/pipeable": "./dist/commonjs/pipeable/index.js"
  },
  "types": "dist/*.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rollup -c",
    "prepublishOnly": "rollup -c && npm t",
    "start": "tsc -w",
    "test": "nyc ava",
    "test:watch": "ava -w",
    "generate-docs": "jsdoc lib README.md -d docs",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "lint": "eslint src --ext .ts",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desnor/itrabble.git"
  },
  "keywords": [
    "iterables",
    "ES6",
    "lazy",
    "streams",
    "generator",
    "iterator",
    "ES2015"
  ],
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {},
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "author": "Alex Revell <revellalex@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/desnor/itrabble/issues"
  },
  "homepage": "https://github.com/desnor/itrabble#readme",
  "size-limit": [
    {
      "path": "dist/index.umd.js",
      "limit": "10 KB"
    },
    {
      "path": "commonjs/index.js",
      "limit": "10 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@ava/typescript": "^3.0.1",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/node": "^15.0.1",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "ava": "4.0.0-rc.1",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^6.0.0",
    "nyc": "^14.1.1",
    "prettier": "^2.3.0",
    "rollup": "^2.46.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^10.0.0",
    "size-limit": "^4.10.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
